{
  "name": "NY Times decline login",
  "description": "Hide all the content blockers. Allows to read The New York Times articles without login popup",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128": "images/icon-128.png",
    "64": "images/icon-64.png",
    "48": "images/icon-48.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.nytimes.com/*"]
    }
  ],
  "action": {},
  "permissions": [],
  "background": {}
}
